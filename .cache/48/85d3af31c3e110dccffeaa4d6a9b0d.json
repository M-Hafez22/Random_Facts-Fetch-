ute;\r\n\t\t\tleft: 0;\r\n\t\t\tbottom: 0;\r\n\t\t\theight: 0%;\r\n\t\t\twidth: 100%;\r\n\t\t\tbackground: $clr;\r\n\t\t\topacity: .30;\r\n\t\t\tborder-radius: 5px;\r\n\t\t\ttransition:  all .75s ease;\r\n\t\t}\r\n\r\n\t\t& .content{\r\n\t\t\tfont-size: 1.25em;\r\n\t\t\twidth: 100%; \r\n\t\t\tposition: absolute;\r\n\t\t\tbottom: 5px;\r\n\t\t\tleft: 0;\r\n\t\t\tpadding-left: 1em;\r\n\t\t\ttransition:  all .5s ease;\r\n\t\t}\r\n\t}\r\n}","styles\\_config.scss":"$clr: #443005;\r\n$bg-clr: #fff;\r\n","styles\\_type.scss":"/*  --Map--\r\n.type\r\n\t.header\r\n\t.buttons\r\n\t\t.btn\r\n\t\t.active\r\n*/\r\n.type{\r\n  width: 100%;\r\n\r\n  &__header{\r\n  \ttext-transform: capitalize; \r\n  \tfont-size: 2em;\r\n  \tcolor: darken($clr, 15%);\r\n  }\r\n\r\n  &__buttons{\r\n  \tdisplay: flex;\r\n\t  flex-direction: row; \r\n\t  justify-content: space-around;\r\n\t  align-items: center;\r\n\r\n\t  &__btn{\r\n\t  \tposition:relative ;\r\n      color:  $clr;\r\n      font-size: 1.25em;\r\n      overflow: hidden;\r\n      padding: 1rem;\r\n      text-transform: capitalize;\r\n\t    border: 2px solid currentColor;\r\n\t    border-radius: 15%;\r\n\r\n\t    &:hover::after{\r\n\t      transform: scaleX(1);\r\n\t      transform-origin: left;\r\n\t    \t}\r\n\r\n      &::after{\r\n        content: '';\r\n\t      position:absolute ;\r\n\t      left: 0;\r\n\t      top: 0;\r\n\t      height:100%;\r\n\t      width: 100%;\r\n\t      background:  rgba( $clr ,0.5);\r\n\t      z-index: 1;\r\n\t      transform: scaleX(0);\r\n\t      transform-origin: right ;\r\n\t      transition:  transform 1s ease;\r\n    \t}\r\n\t  }\r\n\t  .active{\r\n\t\t\tbackground: $clr;\r\n\t\t\tcolor: lighten($clr, 70%);\r\n\t\t}\r\n  }\r\n}\r\n\r\n\r\n","styles\\_fact.scss":"\r\n.fact{\r\n\tdisplay: none;\r\n\twidth: 90%;\r\n\tmargin: auto;\r\n\r\n\t&__number{}\r\n\r\n\t&__text{\r\n\t\tfont-size: 1.25em;\r\n\t\tcolor: darken($clr, 25%);\r\n\t\ttext-align: left;\r\n\t\tbackground: lighten($clr, 65%);\r\n\t\tborder-radius: 15px;\r\n\t\tpadding: .5em;\r\n\r\n\t}\r\n}","styles\\_loader.scss":".loaders{display: none;}\r\n\r\n.loader,\r\n.loader:before,\r\n.loader:after {\r\n  border-radius: 50%;\r\n  width: 2.5em;\r\n  height: 2.5em;\r\n  -webkit-animation-fill-mode: both;\r\n  animation-fill-mode: both;\r\n  -webkit-animation: load7 1.8s infinite ease-in-out;\r\n  animation: load7 1.8s infinite ease-in-out;\r\n}\r\n.loader {\r\n  color: $clr;\r\n  font-size: 10px;\r\n  margin: 80px auto;\r\n  position: relative;\r\n  text-indent: -9999em;\r\n  -webkit-transform: translateZ(0);\r\n  -ms-transform: translateZ(0);\r\n  transform: translateZ(0);\r\n  -webkit-animation-delay: -0.16s;\r\n  animation-delay: -0.16s;\r\n}\r\n.loader:before,\r\n.loader:after {\r\n  content: '';\r\n  position: absolute;\r\n  top: 0;\r\n}\r\n.loader:before {\r\n  left: -3.5em;\r\n  -webkit-animation-delay: -0.32s;\r\n  animation-delay: -0.32s;\r\n}\r\n.loader:after {\r\n  left: 3.5em;\r\n}\r\n@-webkit-keyframes load7 {\r\n  0%,\r\n  80%,\r\n  100% {\r\n    box-shadow: 0 2.5em 0 -1.3em;\r\n  }\r\n  40% {\r\n    box-shadow: 0 2.5em 0 0;\r\n  }\r\n}\r\n@keyframes load7 {\r\n  0%,\r\n  80%,\r\n  100% {\r\n    box-shadow: 0 2.5em 0 -1.3em;\r\n  }\r\n  40% {\r\n    box-shadow: 0 2.5em 0 0;\r\n  }\r\n}","styles\\main.scss":"@import 'config';\r\n@import 'input';\r\n@import 'type';\r\n@import 'fact';\r\n@import \"loader\";\r\n\r\n/*\r\n[1] body\r\n[2] .page\r\n[3] header h1\r\n*/\r\n\r\n\r\nbody{\r\n  height: 100vh;\r\n  // overflow: hidden;\r\n  & .page{\r\n    width: 90%;\r\n    height: 90vh;\r\n    margin: auto;\r\n    text-align: center;\r\n    background: $bg-clr; \r\n    border-radius: 15px;\r\n    -webkit-box-shadow: 0px 0px 53px 1px  $clr;\r\n\t\t-moz-box-shadow: 0px 0px 53px 1px  $clr;\r\n\t\tbox-shadow: 0px 0px 53px 1px  $clr;\r\n\r\n    \r\n    h1{\r\n      text-transform : uppercase;\r\n      color: darken($clr, 20%);\r\n      padding: 1rem;\r\n      margin-top: 1rem; \r\n    }\r\n\r\n    main{\r\n      wi